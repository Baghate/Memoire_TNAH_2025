@phdthesis{froissart_union_1990,
	type = {phdthesis},
	title = {"{De} l'union de l'art et de l'industrie": les origines de l'{Union} centrale et la fondation du {Musée} des arts décoratifs de {Paris}},
	shorttitle = {"{De} l'union de l'art et de l'industrie"},
	url = {https://amu.hal.science/tel-03029865},
	abstract = {Se demander par quelles étapes passa la réévaluation progressive des « arts mineurs» signifie s'interroger en premier lieu sur le rôle joué par l'Union centrale des Arts décoratifs, le plus puissant des groupements d'artistes, industriels, collectionneurs, érudits et hauts fonctionnaires de la seconde moitié du XIXe siècle en France. Les origines de l'Union centrale se situent peu avant la première exposition universelle, en 1845, lorsque les artistes industriels Amédée Couder et Édouard Guichard créent une Société du Progrès de l'Art industriel, Société que le publiciste Théodore Labourieu refonde en 1858. Devenue en 1864 l’Union centrale des Beaux-appliqués à l’industrie, cette association d’artistes, de fabricants et de personnalités politiques fusionne en 1881 avec la Société du musée des arts décoratifs (créée en 1877) dans un seul et puissant organisme, l’Union centrale des arts décoratifs. L’Union centrale déploya son activité et exerça son extraordinaire influence dans des directions différentes, parfois contradictoires. Ses intérêts évoluèrent au gré des hommes qui la composèrent et qui la dirigèrent, et ses mutations reflètent souvent le cours des événements politiques ou les écoles de pensée qui marquèrent le siècle. Ce mémoire de maîtrise est la première tentative de reconstituer cette histoire et de circonscrire les discours et les actions qui ont marqué la naissance du musée des Arts décoratifs de Paris.},
	urldate = {2025-04-25},
	school = {Université Paris IV Sorbonne},
	author = {Froissart, Rossella Pezone},
	month = oct,
	year = {1990},
	keywords = {Th},
}

