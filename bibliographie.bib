@inproceedings{bermes_image_2023,
	series = {Humanités numériques et science ouverte},
	title = {L'image comme particule élémentaire, ou les prémisses d'un changement d'échelle à la {BnF}},
	url = {https://hal.science/hal-03991515},
	abstract = {Si le travail sur les images passionne les bibliothécaires et les chercheurs de la Bibliothèque nationale de France depuis des siècles, il a connu un tournant majeur avec le développement des traitements automatisés des documents numériques, qui promettent de faire sortir de l’ombre des images qui n’ont jamais circulé ni fait l’objet de traitements documentaires en tant qu’objets indépendants et autonomes.
Cet article se propose de raconter ces évolutions selon une approche tant historique que technique, en insistant sur la manière dont, à l’issue d’une décennie d’explorations et d’expérimentations appliquées à divers contenus iconographiques, l’établissement s’achemine vers une implantation généralisée de ces fonctionnalités de fouille dans l’ensemble de Gallica, sa bibliothèque numérique et celle de ses partenaires.
Ces nouveaux modes de « lecture » – ou, en l’occurrence, de « vision » – ne laissent pas d’interroger la notion même d’image et tout son écosystème, à commencer par la nature du dialogue qu’entretiennent l’approche visuelle et l’approche textuelle d’un contenu. 
Face aux défis qui se présentent, notamment en termes d’accès à ces contenus de publics toujours plus nombreux et diversifiés, selon des modalités et des canaux étendus, sont également soulevées les questions de la relation homme-machine et de la plus-value des professionnels de l’information.},
	urldate = {2025-09-01},
	booktitle = {The {Measurement} of {Images}. {Computational} {Approaches} in the {History} and {Theory} of the {Arts}, sous la direction de {Clarisse} {Bardiot} et {Emmanuel} {Château}-{Dutier}},
	publisher = {Presses universitaires du Septentrion},
	author = {Bermès, Emmanuelle and Leclaire, Céline and Moreux, Jean-Philippe},
	year = {2023},
	keywords = {Articles, Artificial intelligence, Bibliothèques numériques, Computer vision, Digital libraries, Fouille d'images, Image mining, Intelligence artificielle, Vision par ordinateur},
